{
  "$schema": "https://raw.githubusercontent.com/mathdothings/dotfiles/master/darkminimal.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#5f9dcb",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#dcdcaa",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#dcdcaa>git:(</>{{ .HEAD }}<#dcdcaa>)</>",
          "type": "git"
        },
        {
          "foreground": "#BF616A",
          "style": "plain",
          "template": " \u2717",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}