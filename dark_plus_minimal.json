{
  "$schema": "https://raw.githubusercontent.com/mathdothings/dotfiles/master/dark_plus_minimal.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#4FC1FF",
          "style": "plain",
          "template": " \u279c",
          "type": "text"
        },
        {
          "foreground": "#CE9178",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#4EC9B0",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#C586C0>git:(</#C586C0>{{ .HEAD }}<#C586C0>)</#C586C0>",
          "type": "git"
        },
        {
          "foreground": "#D16969",
          "style": "plain",
          "template": " \u2717",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}